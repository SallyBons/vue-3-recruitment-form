from \"../utils/getMonths\";\nimport { generateUID } from \"../utils/generateUID\";\n\nexport default {\n  name: \"MonthPicker\",\n  props: {\n    title: {\n      type: String,\n      default: \"Place your headline here\"\n    },\n    mandatory: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      isOpened: false,\n      selectedMonth: getMonths()[this.getClientMonth()].value,\n      selectedYear: this.getClientYear(),\n      months: getMonths()\n    };\n  },\n  computed: {\n    guid: function() {\n      return \"MonthPicker-\" + generateUID();\n    },\n    pickerValue: function() {\n      return `${this.selectedMonth} ${this.selectedYear}`;\n    }\n  },\n  methods: {\n    selectMonth(value) {\n      this.selectedMonth = value;\n    },\n    documentClick(e) {\n      let el = this.$refs[this.guid];\n      let target = e.target;\n\n      if (el !== target && !el.contains(target)) {\n        this.isOpened = false;\n      } else if (\n        target.className.includes(\"month-picker__avaliable-months__single\") &&\n        el.contains(target)\n      ) {\n        this.isOpened = false;\n      } else {\n        this.isOpened = true;\n      }\n    },\n    getClientYear() {\n      return new Date().getFullYear();\n    },\n    getClientMonth() {\n      // Returns index of current month from client\n      return new Date().getMonth();\n    },\n    incrementYear() {\n      this.selectedYear += 1;\n    },\n    decrementYear() {\n      this.selectedYear -= 1;\n    }\n  },\n  created() {\n    document.addEventListener(\"click\", this.documentClick);\n  },\n  unmounted() {\n    document.removeEventListener(\"click\", this.documentClick);\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n$max-width: 200px;\n\n.month-picker {\n  &__selected-value {\n    border: 1px solid red;\n  }\n\n  &__avaliable-months {\n    max-width: 200px;\n    display: flex;\n    flex-wrap: wrap;\n\n    &__single {\n      max-width: 25%;\n      width: 100%;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}