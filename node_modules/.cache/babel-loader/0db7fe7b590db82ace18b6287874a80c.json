EAAd,CAArB;AAeJ,GAfN,E,CACE,YAAA,CAaa,qBAbb,EAaa;AAZV,IAAA,OAAO,EAAE,KAAA,CAAA,YAYC;AAXX,IAAA,WAAW,EAAC,KAWD;AAVX,gBAAS,MAUE;AATV,IAAA,UAAQ,EAAE,QAAA,CAAA,kBASA;AARV,IAAA,SAAO,EAAE,QAAA,CAAA;AAQC,GAbb,E;AAOmB,qBAAa,OAAA,CAC5B;AAAA,UADgC,MAChC,QADgC,MAChC;AAAA,aADsC,CACtC,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,YAAA,CAAyD,KAAzD,EAAyD;AAAnD,QAAA,KAAK,gCAAyB,MAAM,CAAC,IAAhC;AAA8C,OAAzD,E,IAAA,E;;AAAA,OAEI,EADJ,YAAA,CAA4C,MAA5C,EAAA,UAAA,EAA4C,gBAAA,CAAtB,MAAM,CAAC,IAAe,CAA5C,EAAiC;AAAA;AAAjC,OACI,CAHN,CADsC,CACtC;AAAA,KAD4B,C;;;;GAPhC,E;;AAAA,I,sCAAA,C,CADF,E;;AAAA,GA0BI,EAVJ,YAAA,CASM,KATN,EAAA,UAAA,EASM,C,gBARJ,YAAA,CAIE,OAJF,EAIE;AAHA,IAAA,KAAK,EAAC,cAGN;AAFA,IAAA,WAAW,EAAC,OAEZ;;aADc,KAAA,CAAA,MAAA,GAAM,M;;AACpB,GAJF,E,IAAA,E;;AAAA,G,iBAGgB,KAAA,CAAA,M;UAAd;MAKE,EAH8B,QAAA,CAAA,aAAA,CAAc,M,kBAAhD,YAAA,CAES,MAFT,EAAA,UAAA,EAES,gBAAA,CADP,QAAA,CAAA,aAAA,CAAc,MACP,CAFT,EACsB;AAAA;AADtB,G,qCAGI,CATN,CAUI,CA3BN,CA4BI,CAhCN,C","sourcesContent":["<template>\n  <div class=\"mobile-tel-component\">\n    <label for=\"input-wrapper\" class=\"input-label\">\n      {{ title }}\n    </label>\n    <div class=\"input-wrapper\">\n      <div class=\"vue-select-wrapper\" :class=\"[code !== '' ? 'active' : '']\">\n        <vue-select\n          :options=\"countryArray\"\n          placeholder=\"Код\"\n          label-by=\"code\"\n          @selected=\"handleCodeSelected\"\n          @removed=\"handleCodeRemoved\"\n        >\n          <template v-slot:dropdown-item=\"{ option }\">\n            <div class=\"flag-and-code-wrapper\">\n              <div :class=\"`flag-icon flag-icon-${option.name}`\"></div>\n              <span class=\"code\">{{ option.code }} </span>\n            </div>\n          </template>\n        </vue-select>\n      </div>\n      <div class=\"input-with-errors-wrapper\">\n        <input\n          class=\"number-input\"\n          placeholder=\"Номер\"\n          v-model.lazy=\"number\"\n        />\n        <span class=\"error-message\" v-if=\"errorMessages.number\">{{\n          errorMessages.number\n        }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport VueSelect from \"vue-next-select\";\nimport { numeric, minLength } from \"@vuelidate/validators\";\nimport { getErrorMessage } from \"../utils/getErrorMessage.js\";\n\nexport default {\n  name: \"MobileTelephoneComponent\",\n  props: {\n    title: String,\n  },\n\n  data() {\n    return {\n      code: \"\",\n      number: \"\",\n      countryArray: [\n        { name: \"ru\", code: \"+7\" },\n        { name: \"by\", code: \"+375\" },\n        { name: \"ua\", code: \"+380\" },\n      ],\n    };\n  },\n  methods: {\n    handleCodeSelected(value) {\n      this.code = value.code;\n    },\n    handleCodeRemoved() {\n      this.code = \"\";\n    },\n    getTelephone() {\n      return String(this.code + this.number);\n    },\n  },\n  computed: {\n    date: function () {\n      return {\n        code: this.code,\n        numder: this.number,\n      };\n    },\n    errorMessages: function () {\n      return {\n        number: getErrorMessage(this.$v.number.$errors),\n      };\n    },\n  },\n  watch: {\n    date: function () {\n      this.$store.dispatch(\"setSecondStepField\", {\n        field: \"number\",\n        value: this.getTelephone(),\n      });\n    },\n  },\n  components: {\n    VueSelect,\n  },\n\n  validations() {\n    return {\n      number: {\n        numeric,\n        minLength: minLength(9),\n        $autoDirty: true,\n      },\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import \"../assets/scss/styledDropdown.scss\";\n</style>\n\n<style lang=\"scss\" scoped>\n@import \"../assets/scss/_variables.scss\";\n\n.mobile-tel-component {\n  display: flex;\n  flex-direction: column;\n  padding: 6px 6px 28px 6px;\n  max-width: 630px;\n  width: 100%;\n  position: relative;\n\n  .input-label {\n    color: $text-grey;\n    font-family: \"Montserrat\", sans-serif;\n    font-style: normal;\n    font-weight: bold;\n    font-size: 12px;\n    line-height: 16px;\n    margin-bottom: 6px;\n  }\n\n  .input-wrapper {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    .input-with-errors-wrapper {\n      position: relative;\n      display: flex;\n      flex-direction: column;\n      max-width: 410px;\n      width: 100%;\n    }\n    .number-input {\n      max-width: 410px;\n      width: 100%;\n      height: 50px;\n      border: 1px solid $main-grey;\n      border-radius: 10px;\n      padding: 0 20px;\n      font-family: \"Inter\", sans-serif;\n      font-style: normal;\n      font-size: 14px;\n      line-height: 17px;\n      box-sizing: border-box;\n      @media (max-width: 375px) {\n        max-width: 160px;\n      }\n      &::placeholder {\n        color: $main-grey;\n        font-weight: bold;\n      }\n\n      &:focus {\n        border-color: $template-color;\n      }\n    }\n  }\n\n  .error-message {\n    font-family: \"Inter\", sans-serif;\n    font-style: normal;\n    font-weight: normal;\n    font-size: 11px;\n    line-height: 15px;\n    color: $mark-red;\n    margin-top: 4px;\n    position: absolute;\n    bottom: -20px;\n  }\n}\n.flag-and-code-wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  max-width: 130px;\n  width: 100%;\n  .flag-icon {\n    background-size: 100%;\n    background-repeat: no-repeat;\n    width: 30px;\n    height: 20px;\n\n    &.flag-icon-ru {\n      background-image: url(\"../assets/images/ru-flag.svg\");\n    }\n    &.flag-icon-by {\n      background-image: url(\"../assets/images/by-flag.svg\");\n    }\n    &.flag-icon-ua {\n      background-image: url(\"../assets/images/ua-flag.svg\");\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}