AAW,EAAC,MAA4B;;aAAP,KAAA,CAAA,GAAA,GAAG,M;;AAAI,GAAjE,E,IAAA,E;;AAAA,G,iBAA0D,KAAA,CAAA,G;UAAd;MAIxC,EAH8B,QAAA,CAAA,aAAA,CAAc,G,kBAAhD,YAAA,CAES,MAFT,EAAA,UAAA,EAES,gBAAA,CADP,QAAA,CAAA,aAAA,CAAc,GACP,CAFT,EACmB;AAAA;AADnB,G,qCAGI,CALN,CAwBI,EAjBJ,YAAA,CAUM,KAVN,EAUM;AAVD,IAAA,KAAK,EAAA,CAAC,oBAAD,EAAqB,CAAU,KAAA,CAAA,KAAA,KAAK,EAAL,GAAK,QAAL,GAAK,EAAf,CAArB;AAUJ,GAVN,E,CACE,YAAA,CAKc,qBALd,EAKc;AAJX,IAAA,OAAO,EAAE,KAAA,CAAA,UAIE;AAHZ,IAAA,WAAW,EAAC,OAGA;AAFX,IAAA,UAAQ,EAAE,QAAA,CAAA,mBAEC;AADX,IAAA,SAAO,EAAE,QAAA,CAAA;AACE,GALd,E,IAAA,E;;AAAA,I,sCAAA,C,EAMkC,QAAA,CAAA,aAAA,CAAc,K,kBAAhD,YAAA,CAES,MAFT,EAAA,UAAA,EAES,gBAAA,CADP,QAAA,CAAA,aAAA,CAAc,KACP,CAFT,EACqB;AAAA;AADrB,G,sCAPF,E;;AAAA,GAiBI,EANJ,YAAA,CAKM,KALN,EAAA,UAAA,EAKM,C,gBAJJ,YAAA,CAAkE,OAAlE,EAAkE;AAA3D,IAAA,KAAK,EAAC,YAAqD;AAAxC,IAAA,WAAW,EAAC,KAA4B;;aAAR,KAAA,CAAA,IAAA,GAAI,M;;AAAI,GAAlE,E,IAAA,E;;AAAA,G,iBAA0D,KAAA,CAAA,I;UAAd;MAIxC,EAH8B,QAAA,CAAA,aAAA,CAAc,I,kBAAhD,YAAA,CAES,MAFT,EAAA,UAAA,EAES,gBAAA,CADP,QAAA,CAAA,aAAA,CAAc,IACP,CAFT,EACoB;AAAA;AADpB,G,qCAGI,CALN,CAMI,CAzBN,CA0BI,CA9BN,C","sourcesContent":["<template>\n  <div class=\"birthday-component\">\n    <label for=\"input-wrapper\" class=\"input-label\">\n      {{ title }}\n    </label>\n    <div class=\"input-wrapper\">\n      <div class=\"input-with-errors-wrapper\">\n        <input class=\"day-input\" placeholder=\"День\" v-model.lazy=\"day\" />\n        <span class=\"error-message\" v-if=\"errorMessages.day\">{{\n          errorMessages.day\n        }}</span>\n      </div>\n\n      <div class=\"vue-select-wrapper\" :class=\"[month !== '' ? 'active' : '']\">\n        <vue-select\n          :options=\"monthArray\"\n          placeholder=\"Месяц\"\n          @selected=\"handleMonthSelected\"\n          @removed=\"handleMonthRemoved\"\n        ></vue-select>\n        <span class=\"error-message\" v-if=\"errorMessages.month\">{{\n          errorMessages.month\n        }}</span>\n      </div>\n      <div class=\"input-with-errors-wrapper\">\n        <input class=\"year-input\" placeholder=\"Год\" v-model.lazy=\"year\" />\n        <span class=\"error-message\" v-if=\"errorMessages.year\">{{\n          errorMessages.year\n        }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport VueSelect from \"vue-next-select\";\nimport { between, minValue, requiredUnless } from \"@vuelidate/validators\";\nimport { getErrorMessage } from \"../utils/getErrorMessage.js\";\n\nexport default {\n  name: \"BirthDayComponent\",\n  props: {\n    title: String,\n  },\n\n  data() {\n    return {\n      day: \"\",\n      month: \"\",\n      year: \"\",\n      monthArray: [\n        \"Январь\",\n        \"Февраль\",\n        \"Март\",\n        \"Апрель\",\n        \"Май\",\n        \"Июнь\",\n        \"Июль\",\n        \"Август\",\n        \"Сентябрь\",\n        \"Октябрь\",\n        \"Ноябрь\",\n        \"Декабрь\",\n      ],\n    };\n  },\n  methods: {\n    handleMonthSelected(value) {\n      this.month = value;\n    },\n    handleMonthRemoved() {\n      this.month = \"\";\n    },\n  },\n  computed: {\n    date: function () {\n      return {\n        day: this.day,\n        month: this.month,\n        year: this.year,\n      };\n    },\n    errorMessages: function () {\n      return {\n        day: getErrorMessage(this.$v.day.$errors),\n        month: getErrorMessage(this.$v.month.$errors),\n        year: getErrorMessage(this.$v.year.$errors),\n      };\n    },\n  },\n  watch: {\n    date: function () {\n      this.$store.dispatch(\"setSecondStepField\", {\n        field: \"birthDayDate\",\n        value: this.date,\n      });\n    },\n  },\n  components: {\n    VueSelect,\n  },\n\n  validations() {\n    return {\n      day: {\n        between: between(1, 31),\n        $autoDirty: true,\n      },\n      month: {\n        required: requiredUnless(this.day === \"\" || this.year === \"\"),\n        $autoDirty: true,\n      },\n      year: {\n        minValue: minValue(1900),\n        $autoDirty: true,\n      },\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\n@import \"../assets/scss/styledDropdown.scss\";\n</style>\n\n<style lang=\"scss\" scoped>\n@import \"../assets/scss/_variables.scss\";\n\n.birthday-component {\n  display: flex;\n  flex-direction: column;\n  padding: 6px 6px 28px 6px;\n  max-width: 630px;\n  width: 100%;\n  position: relative;\n  box-sizing: border-box;\n  .input-label {\n    color: $text-grey;\n    font-family: \"Montserrat\", sans-serif;\n    font-style: normal;\n    font-weight: bold;\n    font-size: 12px;\n    line-height: 16px;\n    margin-bottom: 6px;\n  }\n\n  .input-wrapper {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    .input-with-errors-wrapper {\n      position: relative;\n      display: flex;\n      flex-direction: column;\n    }\n    .day-input,\n    .year-input {\n      max-width: 190px;\n      width: 100%;\n      height: 50px;\n      border: 1px solid $main-grey;\n      border-radius: 10px;\n      padding: 0 20px;\n      font-family: \"Inter\", sans-serif;\n      font-style: normal;\n      font-size: 14px;\n      line-height: 17px;\n      box-sizing: border-box;\n      @media (min-width: 320px) and (max-width: 375px) {\n        padding: 0 15px;\n      }\n      &::placeholder {\n        color: $main-grey;\n        font-weight: bold;\n      }\n\n      &:focus {\n        border-color: $template-color;\n      }\n    }\n  }\n\n  .error-message {\n    font-family: \"Inter\", sans-serif;\n    font-style: normal;\n    font-weight: normal;\n    font-size: 11px;\n    line-height: 15px;\n    color: $mark-red;\n    margin-top: 4px;\n    position: absolute;\n    bottom: -20px;\n  }\n}\n</style>"],"sourceRoot":""}]}