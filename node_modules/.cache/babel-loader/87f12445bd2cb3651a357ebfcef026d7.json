his.$store.dispatch(\"updateLanguageById\", {\n          id: this.language.id,\n          fields: {\n            level: value\n          }\n        });\n      }\n    }\n  },\n  components: {\n    InputComponent,\n    CustomDropdownComponent,\n    FormDividerComponent\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.languages-wrapper {\n  display: flex;\n  .custom-dropdown-component {\n    margin-left: 30px;\n  }\n  @media (max-width: 375px) {\n    flex-direction: column;\n  }\n}\n</style>\n"],"sourceRoot":""}]}